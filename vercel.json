{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "css/**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "images/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "img/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "medium/css/**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "medium/js/**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "medium/images/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "js/**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "fontawesome-free-5.12.1-web/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/app.js",
      "dest": "/app.js"
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/img/(.*)",
      "dest": "/img/$1"
    },
    {
      "src": "/medium/css/(.*)",
      "dest": "/medium/css/$1"
    },
    {
      "src": "/medium/js/(.*)",
      "dest": "/medium/js/$1"
    },
    {
      "src": "/medium/images/(.*)",
      "dest": "/medium/images/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/fontawesome-free-5.12.1-web/(.*)",
      "dest": "/fontawesome-free-5.12.1-web/$1"
    },
    {
      "src": "/fontawesome-free-5.12.1-web/css/(.*)",
      "dest": "/fontawesome-free-5.12.1-web/css/$1"
    },
    {
      "src": "/fontawesome-free-5.12.1-web/js/(.*)",
      "dest": "/fontawesome-free-5.12.1-web/js/$1"
    },
    {
      "src": "/fontawesome-free-5.12.1-web/less/(.*)",
      "dest": "/fontawesome-free-5.12.1-web/less/$1"
    },
    {
      "src": "/fontawesome-free-5.12.1-web/scss/(.*)",
      "dest": "/fontawesome-free-5.12.1-web/scss/$1"
    },
    {
      "src": "/fontawesome-free-5.12.1-web/webfonts/(.*)",
      "dest": "/fontawesome-free-5.12.1-web/webfonts/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/battleofwits",
      "dest": "/battleofwits.html"
    },
    {
      "src": "/battleofwits.html",
      "dest": "/battleofwits.html"
    },
    {
      "src": "/blogs",
      "dest": "/blogs.html"
    },
    {
      "src": "/blogs.html",
      "dest": "/blogs.html"
    },
    {
      "src": "/contact",
      "dest": "/contact.html"
    },
    {
      "src": "/contact.html",
      "dest": "/contact.html"
    },
    {
      "src": "/enchantopia",
      "dest": "/enchantopia.html"
    },
    {
      "src": "/enchantopia.html",
      "dest": "/enchantopia.html"
    },
    {
      "src": "/events",
      "dest": "/events.html"
    },
    {
      "src": "events.html",
      "dest": "/events.html"
    },
    {
      "src": "/gameoflores",
      "dest": "/gameoflores.html"
    },
    {
      "src": "/gameoflores.html",
      "dest": "/gameoflores.html"
    },
    {
      "src": "/nostalgia",
      "dest": "/nostalgia.html"
    },
    {
      "src": "/nostalgia.html",
      "dest": "/nostalgia.html"
    },
    {
      "src": "/ragend",
      "dest": "/ragend.html"
    },
    {
      "src": "/ragend.html",
      "dest": "/ragend.html"
    }
  ]
}